{
  "name": "cmstop-mammoth",
  "version": "1.4.21",
  "author": "zhang1024899756",
  "description": "Convert Word documents from docx to simple HTML and Markdown",
  "keywords": [
    "docx",
    "html",
    "office",
    "word",
    "markdown",
    "md"
  ],
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhang1024899756/cmstop-mammoth.git"
  },
  "dependencies": {
    "argparse": "~1.0.3",
    "bluebird": "~3.4.0",
    "dingbat-to-unicode": "^1.0.1",
    "jszip": "^3.7.1",
    "lop": "^0.4.1",
    "path-is-absolute": "^1.0.0",
    "sax": "~1.1.1",
    "underscore": "^1.13.1",
    "xmlbuilder": "^10.0.0"
  },
  "devDependencies": {
    "browserify": "~13.0.1",
    "browserify-prepend-licenses": "~1.0.0",
    "duck": "^0.1.12",
    "eslint": "2.13.1",
    "hamjest": "2.13.0",
    "http-server": "^14.1.0",
    "mocha": "~2.2.5",
    "prettier": "^2.6.2",
    "temp": "^0.9.4",
    "uglify-js": "~2.4.8",
    "watchify": "^4.0.0"
  },
  "browser": {
    "./lib/unzip.js": "./browser/unzip.js",
    "./lib/docx/files.js": "./browser/docx/files.js"
  },
  "bin": {
    "mammoth": "bin/mammoth"
  },
  "scripts": {
    "dev": "node bin/dev",
    "build:browser": "browserify lib/index.js --standalone mammoth > mammoth.browser.js",
    "build": "npm run build:browser && uglifyjs mammoth.browser.js > mammoth.browser.min.js",
    "pretest": "eslint lib test",
    "test": "mocha test/**/*.tests.js"
  },
  "license": "BSD-2-Clause"
}
